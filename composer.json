{
    "name": "phpspec/magespec",
    "description": "Magento PHPSpec2 extension",
    "require": {
        "phpspec/phpspec2": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimun-stability": "dev",
    "autoload": {
        "psr-0": {
            "PHPSpec2": "src/",
            "": ["vendor/magento/lib","vendor/magento/app"],
            "Mage": "vendor/magento/app/code/core"
        }
    },
