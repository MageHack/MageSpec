{
    "name": "magetest/magento-phpspec-extension",
    "description": "Magento PHPSpec2 extension",
    "require": {
        "phpspec/php-diff": "*@dev",
        "phpspec/phpspec2": "dev-develop",
        "symfony/dependency-injection": "*",
        "symfony/config": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimun-stability": "dev",
    "autoload": {
        "psr-0": {
            "MageTest\\PHPSpec2\\MagentoExtension": "src",
            "": [
                "vendor/magento/lib",
                "vendor/magento/app"
            ],
            "Mage": "vendor/magento/app/code/core"
        }
    },
    "authors": [
        {
            "name": "Alistair Stead",
            "email": "astead@sessiondigital.com"
        },
